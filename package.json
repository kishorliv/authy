  "main": "src/server.js",
  "scripts": {
    "server:watch": "cross-env NODE_PATH=. NODE_ENV=development nodemon --exec babel-node ./src/server.js",
    "server:build": "cross-env NODE_PATH=. NODE_ENV=production babel ./src --out-dir dist/ --copy-files",
    "server": "cross-env NODE_PATH=. NODE_ENV=production node ./dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "test": "cross-env NODE_PATH=. NODE_ENV=test jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "cross-env NODE_PATH=. jest -i --watchAll",
    "coverage": "cross-env NODE_PATH=. jest -i --coverage"
  },
  "keywords": [],
